{
  "name": "bexvibi/laravel-visitor-statistics",
  "description": "Visitor tracker and statistics for Laravel 5",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vildan Bina",
      "email": "vildan.bina@bexeo.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^7.15",
    "illuminate/http": "^7.15",
    "illuminate/database": "^7.15",
    "illuminate/bus": "^7.15",
    "illuminate/queue": "^7.15",
    "illuminate/contracts": "^7.15",
    "piwik/device-detector": "^3.12",
    "guzzlehttp/guzzle": "^6.5",
    "doctrine/dbal": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 | ^7.0 | ^8.0",
    "orchestra/testbench": "^3.5 | ^3.6 | ^3.7 | ^3.8"
  },
  "autoload": {
    "psr-4": {
      "bexvibi\\Laravel\\VisitorTracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bexvibi\\Laravel\\VisitorTracker\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "bexvibi\\Laravel\\VisitorTracker\\VisitorTrackerServiceProvider"
      ],
      "aliases": {
        "VisitStats": "bexvibi\\Laravel\\VisitorTracker\\Facades\\VisitStats"
      }
    }
  }
}
